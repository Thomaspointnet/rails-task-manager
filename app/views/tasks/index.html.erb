<h1>My tasks</h1>

<ul>
  <% @tasks.each do |task| %>
    <li><%= task.title %> <%= link_to "See details", task_path(task) %>
        <%= link_to "Delete", task_path(task), method: :delete, data: { confirm: "Are you sure?" } %></li>
  <% end %>
</ul>
<h4><%= link_to "Create a new task", new_task_path %></h4>
